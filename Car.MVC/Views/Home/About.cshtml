@model About

@{
    ViewData["Title"] = "About";
}

<h2>@Model.title</h2>
<div class="alert alert-danger">@Model.description</div>

<ul>
@foreach (var tags in Model.tags)
{
    <li>@tags</li>
}
</ul>

<div class="container text-center">
    <div class="row">
        <div class="col-sm-4 border">
            Column
        </div>
        <div class="col-sm-1 border">
            Column
        </div>
        <div class="col-sm-1 border">
            Column
        </div>
        <div class="col-sm-6 border">
            Column
        </div>
    </div>
</div>


